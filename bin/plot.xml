<?xml version="1.0" encoding="UTF-8"?>
<config>

	<Data name="dd" url="signal_cocktail_export.root" />
	<Data name="c" url="MTD_Scaled_STAR.root" />

	<Margins left="0.10" right="0.01" top="0.05" bottom="0.12" />

	<TLatex x="0.65" y="0.91" text="Run15 pp at #sqrt{{s}} = 200 GeV" size="0.03"/>

	<TLatex x="0.65" y="0.85" text="|Y_{#mu#mu}| < 0.5 & p_{T}^{#mu#mu} > 2.2 (GeV/c)" size="0.03"/>
	<TLatex x="0.65" y="0.81" text="|#eta_{#mu}| < 0.5 & p_{T}^{#mu} > 1.1 (GeV/c)" size="0.03"/>

	<Transform>
		<Scale save_as="scaled_sig" name="dd/corr_signal" factor="3.54e8" />
	</Transform>

	<Plot>
		<!-- <Axes lsx="0, 5, 100" yr="1e-11, 1e-8, 1000" logy="1" /> -->
		

		

		<Histo name="Smooth_ccbar_mumu" data="dd" style="smooth" title="" xtitle="M_{#mu#mu} (GeV/c^{2})" ytitle="dN/dM (GeV/c^{2})^{-1}" >
			<style draw="hist c" linecolor="#0B99BC" yr="5e-12, 1e-5" xr="0, 4.0" />
		</Histo>


		<Histo name="Scaled_eta_mumu" data="dd" style="smooth" >
			<style draw="same hist c" linecolor="#83BF17" max="1e-3" min="1e-10" xr="0, 3.6"/>
		</Histo>

		<Histo name="Scaled_jpsi_mumu" data="c" style="smooth" >
			<style draw="same hist c" linecolor="#D14D28" />
		</Histo>

		
		<Histo name="Scaled_omega_mumu" data="dd" style="smooth" >
			<style draw="same hist c" linecolor="#0000FF" />
		</Histo>
		<Histo name="Scaled_omega_pi0mumu" data="c" style="smooth" >
			<style draw="same hist c" linecolor="#59C8DF" />
		</Histo>
		<Histo name="Scaled_rho_mumu" data="dd" style="smooth" >
			<style draw="same hist c" linecolor="#737495" />
		</Histo>
		<Histo name="Scaled_phi_mumu" data="dd" style="smooth" >
			<style draw="same hist c" linecolor="#D14D28" />
		</Histo>
		<Histo name="Scaled_eta_gammamumu" data="dd" style="smooth" >
			<style draw="same hist c" linecolor="#ff703f" />
		</Histo>
		<Histo name="Scaled_etaprime_gammamumu" data="dd" style="smooth" >
			<style draw="same hist c" linecolor="#73503C"/>
		</Histo>


		<Histo name="hsum" data="dd" style="smooth"  >
			<style draw="same hist c" linecolor="black"   />
		</Histo>

		
		<Histo name="corr_signal" data="dd" draw="same h" markersize="1.3" markerstyle="29" markercolor="#FF0000" linecolor="#FF0000" />


		
		
		

		<Legend border_size="0" columns="2" >
			<Position pos="top left" padding="0.01, 0.0, 0.0, 0.01" />
			<Entry name="Scaled_etaprime_gammamumu" title="#eta' #rightarrow#gamma#mu#mu" opt="l" />
			<Entry name="Scaled_eta_gammamumu" title="#eta#rightarrow#gamma#mu#mu" opt="l" />
			<Entry name="Scaled_omega_pi0mumu" title="#omega#rightarrow#pi^{0}#mu#mu" opt="l" />
			<Entry name="Scaled_eta_mumu" title="#eta#rightarrow#mu#mu" opt="l" />
			<Entry name="Scaled_phi_mumu" title="#phi#rightarrow#mu#mu" opt="l" />
			<Entry name="Scaled_rho_mumu" title="#rho#rightarrow#mu#mu" opt="l" />
			<Entry name="Scaled_omega_mumu" title="#omega#rightarrow#mu#mu" opt="l" />
			<Entry name="Smooth_ccbar_mumu" title="c#bar{c}#rightarrow#mu#mu" opt="l" />
			<Entry name="Scaled_jpsi_mumu" title="J/#Psi#rightarrow#mu#mu" opt="l" />
			<Entry name="corr_signal" title="data" opt="pe" />
		</Legend>


		<Export url="Cocktail.pdf" />
	</Plot>


	<Plot>
		<Histo name="dd/raw_signal" draw="h" linecolor="#0000FF" title="" logy="0" xr="0, 3.5" linewidth="2" markersize="1" markercolor="#0000FF" markerstyle="8" />
		<Histo name="dd/background" draw="h same" linecolor="#FF0000" linewidth="2" markersize="1" markercolor="#FF0000" markerstyle="8" />
		<!-- <Histo name="scaled_sig" draw="h same" linecolor="#000000" linewidth="2" markersize="1" markercolor="#000000" markerstyle="8" /> -->
		<Export url="raw_sig_bg.pdf" />
	</Plot>


	<smooth draw="hist c" linewidth="2" linecolor="black" optstat="0" logy="1"/>




</config>

