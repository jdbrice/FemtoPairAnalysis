<?xml version="1.0" encoding="UTF-8"?>
<config>
	<Data name="d" url="res.root" />

	<TCanvas width="800" height="700" />

	<Margins>0.05, 0.05, 0.11, 0.11</Margins>
	<Transform>
		<Divide save_as="SoverB_rb" nameA="d/sig_rb" nameB="d/bgs_rb" />
		<Divide save_as="SoverB" nameA="d/sig" nameB="d/bgs" />
	</Transform>
	

	<Plot>
		<Histo name="pcand" draw="hpe" logy="1" min="1" xr="0, 4.0" optstat="0" lw="2" title=""/>
		<Histo name="bgs" draw="same hpe" lw="2"/>
		<Histo name="sig" draw="same hpe" lc="black" lw="2"/>
		<Export url="export_sig_fps.pdf" />
	</Plot>

	<Plot>
		<Histo name="pcand_rb" draw="hpe" logy="1" min="1" xr="0, 4.0" optstat="0" lw="2" title=""/>
		<Histo name="bgs_rb" draw="same hpe" lw="2"/>
		<Histo name="sig_rb" draw="same hpe" lc="black" lw="2"/>
		<Export url="export_sig_fps_rb.pdf" />
	</Plot>

	<Plot>
		<Histo name="SoverB_rb" draw="h" logy="1" lw="2" />
		<Export url="export_sig_over_bg_rb.pdf" />
	</Plot>

	<Plot>
		<Histo name="SoverB" draw="h" logy="1" lw="2" />
		<Export url="export_sig_over_bg.pdf" />
	</Plot>

</config>